User = game.Players.LocalPlayer

Combat:Slider{
	Name = "Slap Aura",
	Default = 0,
	Min = 0,
	Max = 20,
	Description = "Slaps every player near you. This is experimental, it may kick you.",
	Callback = function(Value) 
		getgenv().Range = Value
		if getgenv().Range == 0 then
			for i,v in pairs(User.Character:GetChildren()) do
				if v.ClassName == "Tool" and v:FindFirstChild("Glove") then
					v.Glove.CanTouch = true
                   		end
			end
			for i,v in pairs(User.Backpack:GetChildren()) do
				if v.ClassName == "Tool" and v:FindFirstChild("Glove") then
					v.Glove.CanTouch = true
                   		end
			end
		end
	end
}

function usingParry(player)
	for i,v in pairs(player.Character.Humanoid:GetPlayingAnimationTracks()) do
		if v.Animation.AnimationId == "rbxassetid://16102819260" then
			return true
		end
	end
end

if not getgenv().slapAura then
	task.spawn(function()
		getgenv().slapAura = true
		getgenv().Range = 0
		while wait(0.1) do
			if getgenv().Range > 0 then
				for i,v in pairs(User.Character:GetChildren()) do
					if v.ClassName == "Tool" and v:FindFirstChild("Glove") then
						v.Glove.CanTouch = false
                   			end
				end
				for i,v in pairs(game.Players:GetChildren()) do
					if v ~= User and User.Character:FindFirstChild("HumanoidRootPart") then
						if v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Ragdolled") and not v.Character.HumanoidRootPart:FindFirstChild("BlockedShield") and not v.Character:FindFirstChild("Counterd") and not v.Character:FindFirstChild("Mirage") and not v.Character:FindFirstChild("rock") and not usingParry(v) and (not v.Character:FindFirstChild("Reversed") or User.leaderstats.Glove.Value == "Error") and v.Character.Ragdolled.Value == false and v.Character.isInArena.Value == true then
							Magnitude = (User.Character.HumanoidRootPart.Position - v.Character.HumanoidRootPart.Position).Magnitude
                        				if Magnitude <= getgenv().Range then
								if User.Character:FindFirstChild("Default") or User.Backpack:FindFirstChild("Default") then
									game.ReplicatedStorage.b:FireServer(v.Character.HumanoidRootPart)
								elseif slapRemotes[User.leaderstats.Glove.Value] then
									slapRemotes[User.leaderstats.Glove.Value]:FireServer(v.Character.HumanoidRootPart, true)
								elseif User.leaderstats.Glove.Value == "Boxer" then
									game.ReplicatedStorage.Events.Boxing:FireServer(v.Character, true)
								else
									game.ReplicatedStorage.GeneralHit:FireServer(v.Character.HumanoidRootPart, true)
								end
								for i,g in pairs(User.Character:GetChildren()) do
									if g.ClassName == "Tool" and g:FindFirstChild("Glove") then
										g:Activate()
                   							end
								end
								wait(0.1)
							end
						end
					end
				end
			end
		end
	end)
end

Combat:Button{
	Name = "Godmode",
	Description = "Makes you invincible to slaps and most abilities." ,
	Callback = function()
		GUI:Prompt{
		Followup = false,
		Title = "Enable Godmode",
		Text = "This will reset you, and may break a few gloves. Your glove also won't be seen by others.",
		Buttons = {
		Yes = function()
			if not User.Character:FindFirstChild("entered") then
				firetouchinterest(User.Character:WaitForChild("HumanoidRootPart"), workspace.Lobby.Teleport1, 0)
				firetouchinterest(User.Character:WaitForChild("HumanoidRootPart"), workspace.Lobby.Teleport1, 1)
			end
			repeat wait() until User.Character:FindFirstChildOfClass("Tool") or User.Backpack:FindFirstChildOfClass("Tool")
			User.Character.HumanoidRootPart.CFrame = workspace.SafeSpot.CFrame * CFrame.new(0, 19, 0) 
			wait(0.5)
			for i,v in pairs(User.Character:GetChildren()) do
				if v.ClassName == "Tool" and v:FindFirstChild("Glove") then
					v.Parent = game.LogService
				end
			end
			for i,v in pairs(User.Backpack:GetChildren()) do
				if v.ClassName == "Tool" and v:FindFirstChild("Glove") then
					v.Parent = game.LogService
    				end
			end
			User.Character.Humanoid.Health = 0
			wait(3.8)
			for i,v in pairs(game.LogService:GetChildren()) do
    				v.Parent = User.Backpack
			end
			User.Character.HumanoidRootPart.CFrame = CFrame.new(-4, -5, 2)	
		end,
		No = function()
			return
		end
		}
		}
	end
}

if not getgenv().Cooldown then
	getgenv().Cooldown = 999
	getgenv().Countdown = false
end

Combat:Toggle{
	Name = "Ability Spam",
	StartingState = false,
	Description = "Spams the ability of the glove you're currently using, do not toggle quickly.",
	Callback = function(Value) 
		getgenv().abilitySpam = Value
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Admin" do
			game.ReplicatedStorage.AdminAbility:FireServer(adminAbility)
			wait()
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Berserk" do
			if User.Character:FindFirstChild("Humanoid") and User.Backpack:FindFirstChild("Berserk") then
				User.Character.Humanoid:EquipTool(User.Backpack.Berserk)
			end
			game.ReplicatedStorage.BerserkCharge:FireServer(User.Character.Berserk)
			wait()
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Bind" do
			local players = {}
			for i, v in pairs(game.Players:GetChildren()) do
				if v ~= User and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Ragdolled") and not v.Character.HumanoidRootPart:FindFirstChild("BlockedShield") and not v.Character:FindFirstChild("Counterd") and not v.Character:FindFirstChild("Mirage") and not v.Character:FindFirstChild("rock") and not not v.Character:FindFirstChild("Reversed") and not usingParry(v) and v.Character.Ragdolled.Value == false and v.Character.isInArena.Value == true then
					table.insert(players, v)
				end
			end
			if #players ~= 0 then
				local target = players[math.random(#players)]
				game.ReplicatedStorage.GeneralAbility:FireServer("default",{goal = target.Character.HumanoidRootPart.CFrame, origin = target.Character.HumanoidRootPart.CFrame * CFrame.new(0,5,0)})
			end
			wait()
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Bomb" do
			if getgenv().Cooldown >= 31 or getgenv().cooldownGlove ~= User.leaderstats.Glove.Value then
				getgenv().Cooldown = 0
				game.ReplicatedStorage.BombThrow:FireServer()
			end
			getgenv().cooldownGlove = User.leaderstats.Glove.Value
			wait(0.1)
			getgenv().Cooldown = getgenv().Cooldown + 1
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Boogie" do
			game.ReplicatedStorage.BoogieBall:FireServer()
			wait()
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Boxer" do
			game.ReplicatedStorage.GeneralAbility:FireServer()
			wait()
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Brick" do
			if getgenv().Cooldown >= 11 or getgenv().cooldownGlove ~= User.leaderstats.Glove.Value then
				getgenv().Cooldown = 0
				game.ReplicatedStorage.lbrick:FireServer()
			end
			getgenv().cooldownGlove = User.leaderstats.Glove.Value
			wait(0.1)
			getgenv().Cooldown = getgenv().Cooldown + 1
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "bus" do
			if getgenv().Cooldown >= 51 or getgenv().cooldownGlove ~= User.leaderstats.Glove.Value then
				getgenv().Cooldown = 0
				game.ReplicatedStorage.busmoment:FireServer()
			end
			getgenv().cooldownGlove = User.leaderstats.Glove.Value
			wait(0.1)
			getgenv().Cooldown = getgenv().Cooldown + 1
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Charge" do
			if getgenv().Cooldown >= 31 or getgenv().cooldownGlove ~= User.leaderstats.Glove.Value then
				getgenv().Cooldown = 0
				if User.Character:FindFirstChild("Humanoid") and User.Backpack:FindFirstChild("Charge") then
					User.Character.Humanoid:EquipTool(User.Backpack.Charge)
				end
				game.ReplicatedStorage.GeneralAbility:FireServer(User.Character.Charge)
			end
			getgenv().cooldownGlove = User.leaderstats.Glove.Value
			wait(0.1)
			getgenv().Cooldown = getgenv().Cooldown + 1
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Counter" do
			game.ReplicatedStorage.Counter:FireServer()
			wait()
		end
		while getgenv().abilitySpam and (User.leaderstats.Glove.Value == "Diamond" or User.leaderstats.Glove.Value == "MEGAROCK" or User.leaderstats.Glove.Value == "CUSTOM") do
			game.ReplicatedStorage.Rockmode:FireServer()
			task.wait()
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Divebomb" do
			local players = {}
			for i, v in pairs(game.Players:GetChildren()) do
				if v ~= User and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Ragdolled") and not v.Character.HumanoidRootPart:FindFirstChild("BlockedShield") and not v.Character:FindFirstChild("Counterd") and not v.Character:FindFirstChild("Mirage") and not v.Character:FindFirstChild("rock") and not v.Character:FindFirstChild("Reversed") and not usingParry(v) and v.Character.isInArena.Value == true then
					table.insert(players, v)
				end
			end
			if #players ~= 0 then
				local target = players[math.random(#players)]
				game.ReplicatedStorage.RocketJump:InvokeServer({chargeAlpha = 0, rocketJump = true})
				game.ReplicatedStorage.RocketJump:InvokeServer({explosionAlpha = 1, explosion = true, position = target.Character.HumanoidRootPart.Position})
			end
			wait()
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Dominance" do
			if getgenv().Cooldown >= 31 or getgenv().cooldownGlove ~= User.leaderstats.Glove.Value then
				getgenv().Cooldown = 0
				if User.Character:FindFirstChild("HumanoidRootPart") then
					game.ReplicatedStorage.DominanceAc:FireServer(User.Character.HumanoidRootPart.Position)
				end
			end
			getgenv().cooldownGlove = User.leaderstats.Glove.Value
			wait(0.1)
			getgenv().Cooldown = getgenv().Cooldown + 1
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Draw4" do
			game.ReplicatedStorage.GeneralAbility:FireServer()
			wait()
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Druid" do
			game.ReplicatedStorage.GeneralAbility:FireServer()
			wait()
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Eggler" do
			game.ReplicatedStorage.GeneralAbility:FireServer()
			wait()
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "fish" do
			if getgenv().Cooldown >= 31 or getgenv().cooldownGlove ~= User.leaderstats.Glove.Value then
				getgenv().Cooldown = 0
				game.ReplicatedStorage.GeneralAbility:FireServer()
			end
			getgenv().cooldownGlove = User.leaderstats.Glove.Value
			wait(0.1)
			getgenv().Cooldown = getgenv().Cooldown + 1
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Flamarang" do
			if getgenv().Cooldown >= 61 or getgenv().cooldownGlove ~= User.leaderstats.Glove.Value then
				getgenv().Cooldown = 0
				game.ReplicatedStorage.GeneralAbility:FireServer()
			end
			getgenv().cooldownGlove = User.leaderstats.Glove.Value
			wait(0.1)
			getgenv().Cooldown = getgenv().Cooldown + 1
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Flash" do
			if getgenv().Cooldown >= 21 or getgenv().cooldownGlove ~= User.leaderstats.Glove.Value then
				getgenv().Cooldown = 0
				game.ReplicatedStorage.FlashTeleport:FireServer()
			end
			getgenv().cooldownGlove = User.leaderstats.Glove.Value
			wait(0.1)
			getgenv().Cooldown = getgenv().Cooldown + 1
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Fort" do
			if getgenv().Cooldown >= 31 or getgenv().cooldownGlove ~= User.leaderstats.Glove.Value then
				getgenv().Cooldown = 0
				game.ReplicatedStorage.Fortlol:FireServer()
			end
			getgenv().cooldownGlove = User.leaderstats.Glove.Value
			wait(0.1)
			getgenv().Cooldown = getgenv().Cooldown + 1
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Frostbite" do
			game.ReplicatedStorage.GeneralAbility:FireServer(1)
			wait()
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Glitch" do
			if getgenv().Cooldown >= 41 or getgenv().cooldownGlove ~= User.leaderstats.Glove.Value then
				getgenv().Cooldown = 0
				game.ReplicatedStorage.GeneralAbility:FireServer()
			end
			getgenv().cooldownGlove = User.leaderstats.Glove.Value
			wait(0.1)
			getgenv().Cooldown = getgenv().Cooldown + 1
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Golden" do
			if getgenv().Cooldown >= 51 or getgenv().cooldownGlove ~= User.leaderstats.Glove.Value then
				getgenv().Cooldown = 0
				game.ReplicatedStorage.Goldify:FireServer()
			end
			getgenv().cooldownGlove = User.leaderstats.Glove.Value
			wait(0.1)
			getgenv().Cooldown = getgenv().Cooldown + 1
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Hallow Jack" do
			if getgenv().Cooldown >= 31 or getgenv().cooldownGlove ~= User.leaderstats.Glove.Value then
				getgenv().Cooldown = 0
				game.ReplicatedStorage.Hallow:FireServer()
			end
			getgenv().cooldownGlove = User.leaderstats.Glove.Value
			wait(0.1)
			getgenv().Cooldown = getgenv().Cooldown + 1
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Hexa" do
			game.ReplicatedStorage.GeneralAbility:FireServer()
			wait()
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Jerry" do
			game.ReplicatedStorage.GeneralAbility:FireServer(CFrame.new())
			wait()
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Jester" do
			game.ReplicatedStorage.GeneralAbility:FireServer("Ability1")
			wait()
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Jet" do
			if getgenv().Cooldown >= 51 or getgenv().cooldownGlove ~= User.leaderstats.Glove.Value then
				getgenv().Cooldown = 0
				local players = {}
				for i, v in pairs(game.Players:GetChildren()) do
					if v ~= User and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Ragdolled") and not v.Character.HumanoidRootPart:FindFirstChild("BlockedShield") and not v.Character:FindFirstChild("Counterd") and not v.Character:FindFirstChild("Mirage") and not v.Character:FindFirstChild("rock") and not v.Character:FindFirstChild("Reversed") and not usingParry(v) and v.Character.Ragdolled.Value == false and v.Character.isInArena.Value == true then
						local Magnitude = (User.Character.HumanoidRootPart.Position - v.Character.HumanoidRootPart.Position).Magnitude
						if Magnitude < 100 then
							table.insert(players, v)
						end
					end
				end
				if #players ~= 0 then
					local target = players[math.random(#players)]
					game.ReplicatedStorage.AirStrike:FireServer(target.Character)
					wait(0.1)
					getgenv().Cooldown = getgenv().Cooldown + 1
				end
			end
			getgenv().cooldownGlove = User.leaderstats.Glove.Value
			wait()
		end
		while getgenv().abilitySpam and User.leaderstats.Glove.Value == "Kinetic" do
			if getgenv().Cooldown >= 61 or getgenv().cooldownGlove ~= User.leaderstats.Glove.Value then
				getgenv().Cooldown = 0
				User.Character.Torso.Anchored = true
				for i = 1,100 do
					game.ReplicatedStorage.SelfKnockback:FireServer({["Force"] = -99})
					wait(0.05)
				end
				wait(1)
				User.Character.Torso.Anchored = false
				wait(1)
				if User.Character:FindFirstChild("Humanoid") and User.Backpack:FindFirstChild("Kinetic") then
					User.Character.Humanoid:EquipTool(User.Backpack.Kinetic)
				end
				game.ReplicatedStorage.KineticExpl:FireServer(User.Character.Kinetic)
			end
