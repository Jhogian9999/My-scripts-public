Badges:Toggle{
	Name = "Bob Farm",
	StartingState = false,
	Description = 'Uses Replica ability in the arena, then resets your character.',
	Callback = function(Value) 
		getgenv().bobFarm = Value
		if User.leaderstats.Slaps.Value >= 4000 then
			while getgenv().bobFarm do
				firetouchinterest(User.Character:WaitForChild("HumanoidRootPart"), workspace.Lobby.Teleport1, 0)
				firetouchinterest(User.Character:WaitForChild("HumanoidRootPart"), workspace.Lobby.Teleport1, 1)
				repeat wait() until User.Character:FindFirstChild("entered")
				wait(0.2)
				game.ReplicatedStorage.Duplicate:FireServer()
				User.Character.Humanoid.Health = 0
				repeat wait() until User.Character:FindFirstChild("Humanoid") and User.Character.Humanoid.Health ~= 0
				wait(0.4)
			end
		elseif Value == true then
			GUI:Notification{
			Title = "Error",
			Text = "You must have at least 4,000 slaps.",
			Duration = 3,
			Callback = function() end
			}
		end
	end
}

