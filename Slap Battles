				getgenv().Cooldown = getgenv().Cooldown + 1
			end
			until getgenv().abilitySpam == true or getgenv().Cooldown >= 501
			getgenv().Countdown = false
		end
	end
}

Combat:Toggle{
	Name = "Glove ESP",
	StartingState = false,
	Description = "Shows the glove players are using above their heads.",
	Callback = function(Value) 
		getgenv().gloveESP = Value
		for i, v in pairs(game.Players:GetChildren()) do
			if v.Character and v.Character:FindFirstChild("Head") and v.Character.Head:FindFirstChild("GloveEsp") then
				v.Character.Head.GloveEsp:Destroy()
    			end
		end
		while getgenv().gloveESP do
			for i, v in pairs(game.Players:GetChildren()) do
    				if v ~= User and v.Character and v.Character:FindFirstChild("Head") and v.Character:FindFirstChild("entered") and not v.Character.Head:FindFirstChild("GloveEsp") then
					GloveEsp = Instance.new("BillboardGui", v.Character.Head)
					GloveEsp.Name = "GloveEsp"
					GloveEsp.Size = UDim2.new(0, 100, 0, 150)
					GloveEsp.AlwaysOnTop = true
					GloveEsp.StudsOffset = Vector3.new(0, -1, 0)
					GloveEspText = Instance.new("TextLabel", GloveEsp)
					GloveEspText.BackgroundTransparency = 1
					GloveEspText.Size = UDim2.new(0, 100, 0, 100)
					GloveEspText.TextSize = 20
					GloveEspText.Font = Enum.Font.SourceSansSemibold
					GloveEspText.TextColor3 = Color3.new(1, 1, 1)
					GloveEspText.TextStrokeTransparency = 0
					GloveEspText.TextYAlignment = Enum.TextYAlignment.Center
					if v.Character.IsInDefaultArena.Value == false then
						GloveEspText.Text = "Glove: " .. v.leaderstats.Glove.Value
					else
						GloveEspText.Text = "Glove: Default"
					end
				end
			end
			wait()
		end
	end
}

Combat:Toggle{
	Name = "Auto Enter Arena",
	StartingState = false,
	Description = "Automatically enters the arena when you respawn.",
	Callback = function(Value) 
		getgenv().autoEnterArena= Value
		while getgenv().autoEnterArena do
			if User.Character:FindFirstChild("HumanoidRootPart") and not User.Character:FindFirstChild("entered") then
				firetouchinterest(User.Character:WaitForChild("HumanoidRootPart"), workspace.Lobby.Teleport1, 0)
				firetouchinterest(User.Character:WaitForChild("HumanoidRootPart"), workspace.Lobby.Teleport1, 1)
			end
			wait()
		end
	end
}

Combat:Toggle{
    Name = "Auto find sword",
    StartingState = false,
    Description = "Automatically gets swordmasters swords",
    Callback = function(Value)
        getgenv().AutoSwordClick = Value

        local Players = game:GetService("Players")
        local player = Players.LocalPlayer
        local hrp

        -- Always update Character + HRP
        task.spawn(function()
            while getgenv().AutoSwordClick do
                local char = player.Character or player.CharacterAdded:Wait()
                hrp = char:WaitForChild("HumanoidRootPart")
                task.wait(1)
            end
        end)

        -- Always find the folder even if it respawns
        local function getFolder()
            while getgenv().AutoSwordClick do
                local f = workspace:FindFirstChild("SwordFighter_Swords_")
                if f then return f end
                task.wait(0.5)
            end
        end

        local swords = {}
        local folder

        -- Constantly refresh folder + sword list
        task.spawn(function()
            while getgenv().AutoSwordClick do
                folder = getFolder()
                swords = folder:GetChildren()
                task.wait(1)
            end
        end)

        local function tryTouch(part)
            if not part or not hrp then return end
            for _, child in ipairs(part:GetChildren()) do
                if child:IsA("TouchTransmitter") or child.Name == "TouchInterest" then
                    firetouchinterest(hrp, part, 0)
                    firetouchinterest(hrp, part, 1)
                end
            end
        end

        local function tryClick(part)
            local cd = part:FindFirstChildWhichIsA("ClickDetector")
            if cd then fireclickdetector(cd) end
        end

        -- Main loop (auto fire)
        task.spawn(function()
            while getgenv().AutoSwordClick do
                if hrp and swords then
                    for _, sword in ipairs(swords) do
                        local clickbox =
                            sword:FindFirstChild("Clickbox") or
                            sword:FindFirstChild("clickbox")

                        if clickbox then
                            tryTouch(clickbox)
                            tryClick(clickbox)
                        end
                    end
                end
                task.wait(0.1)
            end
        end)
    end
}
adminAbility = "Anvil"

